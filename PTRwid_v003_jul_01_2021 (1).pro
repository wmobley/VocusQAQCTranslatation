        if (bk2 lt bk1 and bk2 gt 0) then begin
        lamda=180.0/time_cycle
        N0=(bk1-bk2)*exp(par[0]/lamda)
        bk_fit=exp(findgen(n_cycles)/lamda*(-1))*N0+bk2
        endif else bk_fit=fltarr(n_cycles)
