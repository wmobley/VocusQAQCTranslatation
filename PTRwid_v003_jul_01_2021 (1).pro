  endif else  if(Npts gt Thresh2) then data2[Thresh2:Npts-1]=dataX2[Thresh2:Npts-1]
  derivData=deriv(Data2)
  
  if(Npts gt Thresh1) then derivData[0:Thresh1-1]=derdataX0[0:Thresh1-1] else data2=derdataX0
