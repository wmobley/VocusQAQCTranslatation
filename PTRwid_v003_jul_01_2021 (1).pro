                          thresh3=min(eee)/7
     if(max(where(ddd gt 0)) gt -0.5) then thresh2=median(ddd[where(ddd gt 0)]) else thresh2=0
                       
                         ; thresh2=median(ddd)
                          SD1=stddev(ddd[where(ddd lt thresh2)])
